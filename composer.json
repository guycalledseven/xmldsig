{
    "name": "fr3d/xmldsig",
    "description": "Tool for easy management of XML Signatures (http://www.w3.org/TR/xmldsig-core/)",
    "keywords": [
        "xml",
        "xmldsig",
        "signature",
        "security",
        "soap"
    ],
    "license": "BSD-2-Clause",
    "homepage": "https://github.com/Maks3w/xmldsig/",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/guycalledseven/xmlseclibs"
        }
    ],    
    "require": {
        "php": ">= 7.0",
        "ext-dom": "*",
        "ext-soap": "*",
        "robrichards/xmlseclibs": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^6.0",
        "sensiolabs/security-checker": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "FR3D\\XmlDSig\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FR3D\\XmlDSig\\": "test"
        }
    }
}
